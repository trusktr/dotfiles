{
    "name": "vscode-javascript-booster-lang-server",
    "description": "JavaScript Booster Language Server",
    "version": "0.0.1",
    "author": "Stepan Burguchev <me@sburg.net>",
    "license": "MIT",
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.6.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xsburg/vscode-javascript-booster.git"
    },
    "bugs": {
        "url": "https://github.com/xsburg/vscode-javascript-booster/issues"
    },
    "scripts": {
        "installServer": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
        "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
        "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -w -p .",
        "test": "jest",
        "generate-ast-typings": "node ./typings/generate-ast-typings"
    },
    "dependencies": {
        "@babel/parser": "7.4.3",
        "ast-types": "0.12.3",
        "fs-extra": "7.0.1",
        "jscodeshift": "github:xsburg/jscodeshift",
        "lodash": "4.17.11",
        "recast": "0.17.5",
        "vscode-languageserver": "4.1.2",
        "vscode-uri": "1.0.6"
    },
    "devDependencies": {
        "@types/fs-extra": "5.0.5",
        "@types/jest": "22.2.3",
        "@types/lodash": "4.14.123",
        "@types/node": "11.13.0",
        "coveralls": "3.0.3",
        "cpx": "1.5.0",
        "jest": "22.4.3",
        "prettier": "1.16.4",
        "react": "16.8.6",
        "rimraf": "2.6.3",
        "ts-jest": "22.4.5",
        "tslint": "5.15.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-react": "4.0.0",
        "typescript": "3.4.1"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "/tests/.*\\.spec\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
