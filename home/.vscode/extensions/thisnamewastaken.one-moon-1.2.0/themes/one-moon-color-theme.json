  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#7f7f7f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments Variable Name",
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Comments Variable Type",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "DOM Variable, DOM Property, Object Property",
      "scope": "variable.other.property, support.variable.dom.js, variable.other.object.property.js, variable.other.property.js, variable.other.constant.property.js, support.variable.property.dom, variable.other.constant.property.ts, variable.other.constant.property.tsx, support.variable.property.js, support.variable.property.ts, support.variable.property.tsx, variable.other.object.property.ts, variable.other.object.property.tsx, variable.other.property.ts, variable.other.property.tsx, support.variable.property.ts, support.variable.property.tsx, variable.object.property.ts, variable.object.property.tsx, support.class.component.tsx",
