        {
            "name": "Floating-point prefix operators",
            "scope": "source.ocaml, keyword.operator.symbol.prefix.floating-point",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Storage Types",
            "scope": "storage.type, storage.modifier, support.type",
            "settings": {
                "foreground": "#fff190ff"
            }
        },
        {
            "name": "Variant Types",
            "scope": "entity.name.class.variant",
            "settings": {
                "foreground": "#407b93ff"
            }
        },
        {
            "name": "Type name",
            "scope": "entity.other",
            "settings": {
                "foreground": "#d6cdc5ff"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#d78719ff"
            }
        },
        {
            "name": "Type name",
            "scope": "storage.type.user-defined, meta.property-list",
            "settings": {
                "foreground": "#ffd700ff"
            }
