            "settings": {
                "foreground": "#dad688ff"
            }
        },
        {
            "name": "Variant Types",
            "scope": "entity.name.class.variant",
            "settings": {
                "foreground": "#30657fff"
            }
        },
        {
            "name": "Type name",
            "scope": "entity.other",
            "settings": {
                "foreground": "#b5b5b5ff"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#b17415ff"
            }
        },
        {
            "name": "Type name",
            "scope": "storage.type.user-defined, meta.property-list",
            "settings": {
                "foreground": "#d8be13ff"
            }
