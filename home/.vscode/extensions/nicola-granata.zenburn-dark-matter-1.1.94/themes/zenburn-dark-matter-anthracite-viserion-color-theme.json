    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#6e8f6dff"
            }
        },
        {
            "name": "preprocessor",
            "scope": "meta.preprocessor, punctuation.definition.preprocessor",
            "settings": {
                "foreground": "#c29e7bff"
            }
        },
        {
            "name": "phpdoc",
            "scope": "keyword.other.phpdoc",
            "settings": {
                "foreground": "#42713bff"
            }
        },
        {
            "name": "comment punctuation (&#x2f;*, #, &#x2f;&#x2f;)",
            "scope": "punctuation.definition.comment, punctuation.whitespace.comment",
            "settings": {
                "foreground": "#86ac87ff"
            }
        },
        {
            "name": "embedded ruby in string delimiters",
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#6e6e6eff"
            }
        },
        {
            "name": "embedded ruby in string",
            "scope": "source.ruby.embedded",
            "settings": {
                "foreground": "#a97a7bff"
            }
        },
        {
            "name": "Directive",
            "scope": "keyword.other.directive",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#bcbcbcff"
            }
        },
        {
            "name": "Line-number directives",
            "scope": "keyword.other.directive.line-number",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "String",
            "scope": "string, string.unquoted",
            "settings": {
                "foreground": "#b06e6eff"
            }
        },
        {
            "name": "Heredoc",
            "scope": "string.unquoted.heredoc",
            "settings": {
                "foreground": "#b5b5b5ff"
            }
        },
        {
            "name": "Number",
            "scope": "support.constant.numeric, constant.numeric",
            "settings": {
                "foreground": "#71b3b2ff"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant, support.constant",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#b5b593ff"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#a97a7bff"
            }
        },
        {
            "name": "Other constant",
            "scope": "variable.other.constant",
            "settings": {
                "foreground": "#b5b593ff"
            }
        },
        {
            "name": "Language Keyword",
            "scope": "keyword, keyword.control, meta.selector.css, entity.other.attribute-name",
            "settings": {
                "foreground": "#d7b695ff"
            }
        },
        {
            "name": "Instance Variable",
            "scope": "variable.other.readwrite.instance",
            "settings": {
                "foreground": "#d7b695ff"
            }
        },
        {
            "name": "Module Keyword",
            "scope": "entity.name.module, support.other.module",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#d36a0eff"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#c9c9c9ff"
            }
        },
        {
            "name": "Floating-point infix operators",
            "scope": "keyword.operator.symbol.infix.floating-point",
            "settings": {
                "fontStyle": "underline"
            }
